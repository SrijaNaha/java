
import java.util.*;

public class Seconda
{
public static void main(String [] args)
{
Scanner ob = new Scanner(System.in);
System.out.println("Enter the marks obtained in Maths: ");
double m = ob.nextDouble();
System.out.println("Enter the marks obtained in English: ");
double e = ob.nextDouble();
System.out.println("Enter the marks obtained in Science: ");
double s = ob.nextDouble();
System.out.println("Enter the marks obtained in Social Studies: ");
double t = ob.nextDouble();
System.out.println("Enter the marks obtained in Computer Science: ");
double c = ob.nextDouble();

double sum = (m+e+s+t+c)/5;

if(sum >=90)
{
System.out.println("The grade obtained is: A+ ");
}
else if(sum >=80 && sum < 90)
{
System.out.println("The grade obtained is: A ");
}
else if(sum >=70 && sum < 80)
{
System.out.println("The grade obtained is: B+ ");
}
else if(sum >=60 && sum < 70)
{
System.out.println("The grade obtained is: B ");
}
else if(sum >=50 && sum < 60)
{
System.out.println("The grade obtained is: C ");
}
else
{
System.out.println(" Fail ");
}
}
}
